{
  "name": "gorilla",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/gorilla.js",
  "scripts": {
    "build": "browserify ./src/gorilla.js -s Gorilla -t [babelify] -o ./dist/gorilla.js",
    "build:min": "uglifyjs dist/gorilla.js --comments '/!/' -m screw_ie8=true -c screw_ie8=true,unused=false -o dist/gorilla.min.js",
    "watch": "watchify src/gorilla.js -s Gorilla -t [babelify] -o dist/gorilla.js -v",
    "server": "http-server -p 5000 ./ -s",
    "start": "concurrently \"npm run build\" \"npm run watch\" \"npm run server\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floripajs/gorilla.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floripajs/gorilla/issues"
  },
  "homepage": "https://github.com/floripajs/gorilla#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "jest": "^18.0.0",
    "uglify-js": "^2.7.5",
    "watchify": "^3.8.0"
  }
}
